<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miracle.mapper.CommunityMapper">
	<select id="select" parameterType="Integer" resultType="CommunityDto">
		SELECT * FROM community WHERE write_no=#{write_no}
	</select>
	<select id="selectall" resultType="CommunityDto">
		SELECT * FROM community
	</select>
	<insert id="insert" parameterType="CommunityDto">
		INSERT INTO community VALUES({#write_no},#{write_content})
	</insert>
	<delete id="delete" parameterType="Integer">
		DELETE FROM community WHERE write_no=#{write_no}
	</delete>
	<update id="update" parameterType="CommunityDto">
		UPDATE community SET write_content=#{write_content} WHERE write_no=#{write_no}
	</update>
</mapper>